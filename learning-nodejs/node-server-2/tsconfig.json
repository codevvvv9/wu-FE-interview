{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "experimentalDecorators": false,
    "emitDecoratorMetadata": false,
    "removeComments": true
  },
  "exclude": [
    "node_modules",
    "../node_modules",
    "../../node_modules"
  ]
}